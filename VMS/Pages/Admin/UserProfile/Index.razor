@layout AdminLayout

@attribute [Route(Routes.AdminVolunteerProfile + "/{UserId}")]
@attribute [Authorize(Roles = "Admin")]
<div class="row mt-4">
    <div class="col-lg-10 mx-auto px-0">
        <div class="position-relative head-section">
            <h1 class="text-center m-0">Hồ sơ <span>cá nhân</span></h1>
            <a href="@Routes.AdminEditVolunteerProfile/@UserId" class="position-absolute">Chỉnh sửa</a>
        </div>
        <VMS.Pages.UserProflie.Index IsUsedForAdmin="true" UserId="@UserId"/>
    </div>
</div>

@code {
    [Parameter] public string UserId { get; set; }
}
